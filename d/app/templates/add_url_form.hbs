<form>
  <div class="form-group">
    <label for="enter-url">URL Bookmark</label>
    <input type="text" class="form-control" id="url" placeholder="bookmark it!">
  </div>
  <div class="form-group">
    <label for="tag">Tag it</label>
    <input type="text" class="form-control" id="tag" placeholder="We shall call it...">
  </div>
  <button type="submit" class="btn btn-default">Bookmark it!</button>
</form>
